<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.1//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite3.1.dtd">
<urlrewrite>

    <rule>
    	<from>.*</from>
    	<set type="request" name="DOMAIN_PATH">http:127.0.0.1:80</set>
    	<set type="request" name="CONTEXT_PATH">%{context-path}</set>
    </rule>

</urlrewrite>

