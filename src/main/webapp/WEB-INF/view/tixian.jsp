    function verifyAddr(addr) {
        var reg = /[\uFF00-\uFFEF]/;
        var reg_ch = /[\u4E00-\u9FA5\uF900-\uFA2D]/;
        if (reg.test(addr) || reg_ch.test(addr)) {
            return false;
        }
        return true;
    }

    function doSubmit() {
        var addr = $('#addr').val();
        var amount = $('#amount').val();
        var fundpass = $('#fundpass').val();

        if(!$("#makegatecode").prop("checked") && addr == ''){
            noty({text: '提现地址不能为空', type: "warning", layout: "center", theme: 'gateioNotyTheme', timeout: 3000});
            $('#addr').focus();
            return;
        }
        if (!verifyAddr(addr)) {
            noty({
                text: "无效地址，请检查字符格式！",
                type: "warning",
                layout: "center",
                closeWith: ['button', 'click'],
                theme: 'gateioNotyTheme',
                timeout: 3000
            });
            $('#addr').focus();
            return;
        }
        if (amount == '') {
            noty({text: '提现数额不能为空', type: "warning", layout: "center", theme: 'gateioNotyTheme', timeout: 3000});
            $('#amount').focus();
            return;
        }
        if (fundpass == '') {
            noty({text: '资金密码不能为空', type: "warning", layout: "center", theme: 'gateioNotyTheme', timeout: 3000});
            $('#fundpass').focus();
            return;
        }

        $('#submit_btn').attr("disabled", true);

        var form = document.getElementById('withdraw_form');
        form.submit();
    }
