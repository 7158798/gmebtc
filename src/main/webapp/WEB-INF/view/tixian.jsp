        if (num.length - index - 1 > dec) {
            return num.substr(0, index + dec + 1);
        }
        return false;
    }

    function num_fix(num, dec) {
        num = Number(num).toFixed(15);
        var num_fixed = num_need_fix(num, dec);
        return num_fixed ? num_fixed : num;
