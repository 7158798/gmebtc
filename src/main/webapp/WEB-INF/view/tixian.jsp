    function check_number(e,thisVal) {
        var key = e.keyCode, hasP=thisVal.indexOf('.') > -1;
        if (e.ctrlKey || e.altKey) {
            return true;
        }

        if (!( (key >= 48 && key <= 57 )
                        || ( key >= 96 && key <= 105 )
                        || ( key == 8 )   //退格
                        || ( key == 46 ) //Del
                        || ( key == 27 ) //ESC
                        || ( key == 37 ) //左
                        || ( key == 39 ) //右
                        || ( key == 16 ) //shift
                        || ( key == 9 ) //Tab
                        || ( key == 33 ) // page up
                        || ( key == 34 ) // page down
                        || ( key == 190 ) // point
                        || ( key >= 91 && key <= 93 )
                        || ( key >= 110 && key < 124 )
                ) || hasP && key == 190 || hasP && key == 110
        ) {
            return false;
        }

        return true;
    }

    function checkVal(thisVal) {
        var canUse=parseFloat($("#canUse").text()),
            dayLimit=parseFloat($("#dayLimit").text()),
            perMin=parseFloat($("#perMin").text()),
            wd_amount_tip=$("#wd_amount_tip");

        if(thisVal == '')
            wd_amount_tip.text("提现数额不能为空。");
        else if(thisVal < perMin)
            wd_amount_tip.text("提现数额最少"+perMin+"USDT。");
        else if(thisVal > canUse || thisVal > dayLimit)
            wd_amount_tip.text("提现数额超出可用额度。");
        else
            wd_amount_tip.text("");
    }

