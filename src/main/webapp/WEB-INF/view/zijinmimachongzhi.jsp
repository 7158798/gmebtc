<script>
$(document).ready(function (e) {
	$('#smsbtn').click(function() {
        var self = $(this);

        var currentTime = 60;
        var orignialtext = $(this).text();
        var form = self.closest('form');

		self.prop('disabled', true);

        var timer = $.timer(function() {
            currentTime = currentTime - 1;
            self.text(orignialtext + ' (' + currentTime + ')');

            if (currentTime == 0)
            {
                resetTimer();
            }
        }, 1000, false);

        var resetTimer = function()
        {
             timer.stop();
             currentTime = 60;
             self.text(orignialtext).prop('disabled', false);
        }

            var smsid = $('#smsid', form).val();
			var phone = $('#phone', form).val();
            var lang = "cn";
			var msg = "";
			var req = {	
				phonenumber : phone,
				send_pub : '1'
			};
			$.ajax({type:"post",url:"/sms/" + smsid, data:req,
				success : function(data, textStatus){
					var json_res = data;
					if ( json_res && json_res.result ) {
						self.text(orignialtext + ' (' + currentTime + ')');
						timer.play();
						msg = lang == "en" ? "The SMS code has been sent to your cellphone." : "手机短信验证码：" + json_res.msg;
						$('#smscode', form).prop('disabled', false);
					} else {
						 msg = lang == "en" ? "Send SMS faild: "+json_res.msg+", please check your phone number at <a href='/myaccount/sms_setup'>this link</a>." : "短信发送失败："+json_res.msg+"，请到<a href='/myaccount/sms_setup'>这里检查手机号码设置</a>.";
						 //resetTimer();
						 timer.play();
					}
					noty({text: msg, type: "warning",layout: "bottomLeft", timeout: 10000});
				},
				error : function(){
					timer.play();
					noty({text: '网络错误!', type: "warning",layout: "bottomLeft", timeout: 10000});
				},
				complete : function(){
				}
			});

        return false;
    });



    $('#voicebtn').click(function() {
        var self = $(this);

        var currentTime = 60;
        var orignialtext = $(this).text();
        var form = self.closest('form');

		self.prop('disabled', true);

        var timer = $.timer(function() {
            currentTime = currentTime - 1;
            self.text(orignialtext + ' (' + currentTime + ')');

            if (currentTime == 0)
            {
                resetTimer();
            }
        }, 1000, false);

        var resetTimer = function()
        {
             timer.stop();
             currentTime = 60;
             self.text(orignialtext).prop('disabled', false);
        }
            var smsid = $('#smsid', form).val();
			var phone = $('#phone', form).val();
            var lang = "cn";
			var msg = "";
			var req = {	
				phonenumber : phone,
				send_pub : '1'
			};
			$.ajax({type:"post",url:"/voiceverify/" + smsid, data:req,
				success : function(data, textStatus){
					var json_res = data;
					if ( json_res && json_res.result ) {
						self.text(orignialtext + ' (' + currentTime + ')');
						timer.play();
						msg = lang == "en" ? "The voide verification code has been sent to your cellphone." : "电话语音验证码：" + json_res.msg;
						$('#smscode', form).prop('disabled', false);
					} else {
						 msg = lang == "en" ? "Send voice faild: "+json_res.msg+", please check your phone number at <a href='/myaccount/sms_setup'>this link</a>." : "电话语音拨打失败："+json_res.msg+"，请到<a href='/myaccount/sms_setup'>这里检查手机号码设置</a>.";					
						 timer.play();
					}
					noty({text: msg, type: "warning",layout: "bottomLeft", timeout: 10000});
				},
				error : function(){
					timer.play();
					noty({text: '网络错误!', type: "warning",layout: "bottomLeft", timeout: 10000});
				},
				complete : function(){
				}
			});

        return false;
    });
});
</script>




