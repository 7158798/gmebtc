	$.fn.extend({  
		insertContent : function(myValue, t) {  
			var $t = $(this)[0];  
			if (document.selection) {  
				this.focus();  
				var sel = document.selection.createRange();  
				sel.text = myValue;  
				this.focus();  
				sel.moveStart('character', -l);  
				var wee = sel.text.length;  
				if (arguments.length == 2) {  
				var l = $t.value.length;  
				sel.moveEnd("character", wee + t);  
				t <= 0 ? sel.moveStart("character", wee - 2 * t	- myValue.length) : sel.moveStart("character", wee - t - myValue.length);  
				sel.select();  
				}  
			} else if ($t.selectionStart || $t.selectionStart == '0') {  
				var startPos = $t.selectionStart;  
				var endPos = $t.selectionEnd;  
				var scrollTop = $t.scrollTop;  
				$t.value = $t.value.substring(0, startPos) + myValue + $t.value.substring(endPos,$t.value.length);  
				this.focus();  
				$t.selectionStart = startPos + myValue.length;  
				$t.selectionEnd = startPos + myValue.length;  
				$t.scrollTop = scrollTop;  
				if (arguments.length == 2) { 
					$t.setSelectionRange(startPos - t,$t.selectionEnd + t);  
					this.focus(); 
				}  
			} else {                              
				this.value += myValue;                              
				this.focus();  
			}  
		}  
	});


var nickname = $("#nickName").text();
var lastMessage = [];

//chatbox.min
function chatSend(GP,URL,PARAMETERS,RESPONSEFUNCTION){

	$.ajax({
		type: GP,
		url: URL+"/?u=14" + page_rand(),
		data: PARAMETERS,
		xhrFields: { withCredentials: true},
		success: function (data, textStatus) {
			RESPONSEFUNCTION(data);
		},
		error: function (req, textStatus, err) {}
	});
	
}

lastReceived=0;

function rand(min, max) {
	return Math.floor(Math.random() * max + min);
}

function page_rand() {
	return "&c=" + rand(0, 1000000);
}

// firstUpdate 
function firstUpdate(){
	var txid = $("#txid").text();
	chatSend("GET","/pushchat/recv",{lastreceived: lastReceived,txid: txid},firstMessages)
}
// Update info
function updateInfo(){
    var txid = $("#txid").text();
    if(txid != '')
	    chatSend("GET","/pushchat/recv",{lastreceived: lastReceived,txid: txid},showMessages)
}

//First update messages view
function firstMessages(res){
    if(res == ''){
        updateInfo()
    }
	if(res && res.SRVTM) { //如果有新消息
		if (res.SRVTM == lastReceived)
			return;
		
		var txid = $("#txid").text();
		if (!res.txid || res.txid !== txid)
			return;
			
		var msg_html ='';		
		var i;
		var messages = res.messages;
		for (i = 0; messages.length > i; i++) {
			if (messages[i].username == 'System') {
				if(messages[i].msg.indexOf("<em>System:</em>")>= 0) {
					msg_html += "<div class='ucontent sys-uct'><b><i class='system chat-ico' title='系统消息'></i>" + messages[i].msg + "</b></div>";
				}
			} else { 
                var isMe=messages[i].username == 'Self';
				var who='';
                if(isMe) { who='me';} else {who='';}
				msg_html +=  "<div class='uname "+who+"'><span class=chat-userinfo>";
                if(isMe) {
                    msg_html += "<span class='c-time'>"+messages[i].timest+"</span> <span class='unm v1'>我</span>";
                } else {
                    msg_html += "<span class='unm v5'>"+$("#bsName").text()+"</span> <span class='c-time'>"+messages[i].timest+"</span>";
                }
                msg_html += "</span><div><span class='ucontent'><i class='caret'></i>"+messages[i].msg+"</span></div></div>";
			}
		}
		var messagesTag = document.createElement("li");
		messagesTag.innerHTML=msg_html;
		chatBox.appendChild(messagesTag);
		
		if(lastReceived == 0 || chatBox.scrollTop > (chatBox.scrollHeight - chatBox.offsetHeight) - 50)
			chatBox.scrollTop=chatBox.scrollHeight;
			$("#chatBox").animate({ scrollTop: chatBox.scrollHeight }, 200);
		lastReceived=res.SRVTM;

	} else if (res && res.memo && res.memo != '') {
	
		var msg_html ='';
		if (res.memo != '') {
            var itsName=$("#bsName").text();
            msg_html += "<div class='ucontent sys-uct memo-con'><b><i class='memo-icon chat-ico' title="+itsName+"提示>!</i><span>" +itsName+':</span> '+ res.memo + "</b></div><br/>";
		}
		var messagesTag = document.createElement("li");
		messagesTag.innerHTML=msg_html;
		chatBox.appendChild(messagesTag);
	}
}

// Update messages view
var messageNum=0;
function showMessages(res,record){
	if(res && res.SRVTM) { //如果有新消息
		if ($.cookie('show_chat') === undefined || $.cookie('show_chat') === '0') {
			msgTmArr=res.SRVTM;
			if (msgTmArr > 0 && msgTmArr == lastReceived)
				return;
			//messageNum++;
			firstUpdate();
			//$("#chatNum").text(messageNum).css("padding","2px")

		} else { 
		
			if (res.SRVTM == lastReceived)
				return;
						
			var txid = $("#txid").text();
			if (!res.txid || res.txid !== txid)
				return;
						
			var msg_html ='';
		
			var i;
			var messages = res.messages;

            for (i = 0; messages.length > i; i++) {

                var date = new Date();
                var year = date.getFullYear();
                var continues = false;
				var lmsg = '';
                for(var j=0;j<lastMessage.length;j++){
                    try{
                        lmsg = lastMessage[j][0].msg;
                    } catch (e){lmsg = ''}
                    if( messages[i].msg == lmsg && messages[i].username == 'Self' && ( Date.parse(new Date(year + '-' + messages[i].timest)) * 0.001 ) <= lastReceived + 6 ){
                        continues = true;
                        break;
                    }
                }
                if(continues){
                    continue;
                }

				if (messages[i].username == 'System') {
					if(messages[i].msg.indexOf("<em>System:</em>")>= 0) {
						msg_html += "<div class='ucontent sys-uct'><b><i class='system chat-ico' title='系统消息'></i>" + messages[i].msg + "</b></div>";
					}
				} else {
				    var isMe=messages[i].username == 'Self';
					var who='';
                    if(isMe) { who='me';} else {who='';}
					msg_html +=  "<div class='uname "+who+"'>";
					msg_html += "<span class='chat-userinfo'>";
                    if(isMe) {
                        msg_html += "<span class='c-time'>"+messages[i].timest+"</span> <span class='unm v1'>我</span>";
                    } else {
                        msg_html += "<span class='unm v5'>"+$("#bsName").text()+"</span> <span class='c-time'>"+messages[i].timest+"</span>";
                    }
					msg_html += "</span><div><span class='ucontent'><i class='caret'></i>"+messages[i].msg+"</span></div></div>";
				}
			}
			
			var messagesTag = document.createElement("li");
			messagesTag.innerHTML=msg_html;
			chatBox.appendChild(messagesTag);
			
			if(lastReceived == 0 || chatBox.scrollTop > (chatBox.scrollHeight - chatBox.offsetHeight) - 50)
				chatBox.scrollTop=chatBox.scrollHeight;
				$("#chatBox").animate({ scrollTop: chatBox.scrollHeight }, 800);

            if(record !== 'record'){
                lastReceived=res.SRVTM;
            }

		}
	}
}

// Send message
function sendMessage(){

    var txid = $("#txid").html();
    if (txid == '') {
        chatForm.message.value="";
        chatForm.message.focus();
        return;
    }

	var textVal=$("#message").val();
	if(textVal != ''){
		$(".btn_text").hide();
		$(".spinner").show();
	}
	
	var value = $.trim(chatForm.message.value);
	if (value.length > 0) {
	
		if (lastReceived > 0) {
			var data = new Array();
			var now = new Date();
			var month=now.getMonth()+1; 
			var date=now.getDate(); 
			var hour=now.getHours(); 
			var minute=now.getMinutes(); 
			var second=now.getSeconds(); 
			data['txid'] = txid;
			data['SRVTM'] = Date.parse(new Date())*0.001;
			data['messages'] = [{username:'Self',msg:value,timest:month+"-"+date+" "+hour+":"+minute+":"+second}];
			showMessages(data,'record');
			lastMessage.length < 4 ? lastMessage.push(data['messages']) : lastMessage.shift();lastMessage.push(data['messages']);
            chatForm.message.value="";
		}
		
		chatSend("POST","/pushchat/send",{message: value,txid: txid},sentOk);
		chatBox.scrollTop=chatBox.scrollHeight;
	} else {
		chatForm.message.value="";
		chatForm.message.focus();
	}
}

// Sent Ok
function sentOk(res){

	$(".btn_text").show();
	$(".spinner").hide();
	if(res && res.SRVTM > 0){
		chatForm.message.value=""
		chatForm.message.focus()
		chatBox.scrollTop=chatBox.scrollHeight;
	}
	else if(res.msg.indexOf("<em>System:</em>")>= 0) {
		messages=document.createElement("li")
		messages.innerHTML="<div class='ucontent sys-uct'><b><i class='system chat-ico' title='系统消息'></i>" + res.msg + "</b></div>";
		chatBox.appendChild(messages)
		chatBox.scrollTop=chatBox.scrollHeight
	}
	else if(res.msg.indexOf("<em>Bleep:</em>")>= 0) {
