		orderTimer += '<div class=push-service><a href=javascript:contact(' + orderTxid + ',\"' + contactor + '\",\"' + its_phone + '\",\"' + true + '\")>联系对方</a>';
        orderTimer += '<a class="shensu" href="/myaccount/apply_dispute/'+orderTxid+'" onclick="return onApplyDispute('+disTime+');" target="_blank">我要申诉</a>';
		orderTimer += '</div></div></div>';

        if(!_this.next('tr').hasClass('order-dtl')) {//此条目非展开的,此条需要插入dom并展开
