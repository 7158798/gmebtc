                    $(".seller-code,.pay-code").click(function () {
						if ($(this).hasClass('ali-code')) {
							getPaycode(orderTxid, 'alipay');
						}
						else if ($(this).hasClass('wechat-code')) {
							getPaycode(orderTxid, 'wechat');
						}
						
                    }).show();
                }
                if(smallWin){
                    $(".type-select").find("dt").click(function () {
                        $('.'+$(this).attr("data-id")+'-dtl-ul').parent().show().siblings("div").css({"display":"none","margin-right":"0"});
                        $(this).addClass("type-active").siblings().removeClass("type-active");
                    })
