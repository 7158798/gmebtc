                    myPush.parent().prev("table").css("padding-right","0");
                }
                $(window).off('resize');
            });
        }

        if(its_realname.indexOf(" ") > -1){
            contactor=its_realname.replace(/\s/g,'&nbsp;');
        } else {
            contactor=its_realname;
        }

        //订单详情左侧开始
        if(ifBuy) {//买单
            //小屏幕开始
            var typeSelect='', classSmall='', winWidth=document.body.clientWidth;
            if(winWidth < 1600 && showBank && showAli && showWechat) {
                var smallWin=true;
