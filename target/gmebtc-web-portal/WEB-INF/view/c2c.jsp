	//订单详情
    function expandOrder(_this,targetFrom,expendChat,noty,sWinTypeIndex,rowData) {
        var rowNum=_this.attr("data-id");
        if(rowData == undefined || rowNum == undefined) return;

        var orderTxid=rowData[rowNum].txid,
			orderMemo=orderTxid.substr(-5),
            its_realname=rowData[rowNum].its_realname,
			its_phone=rowData[rowNum].its_phone,
            ifBuy=rowData[rowNum].type_buy == 1,
            showBank=rowData[rowNum].show_bank == 1,
            showAli=rowData[rowNum].show_ali == 1,
            showWechat=rowData[rowNum].show_wechat == 1,
            myPush=$("#myPushOrder"),
            dtlBuyer='',dtlTip='',dtlMyInfo='',contactor='',itemDtl='',odSellCon ='',statusColor='', statuaUnPay='';

        var status = rowData[rowNum].status,
            total = rowData[rowNum].total,
			format_total = total.replace(/,/g, ''),			
            op = "--",
            tips = '',statusWarnTip = '',
            cur_symbol = 'USDT_CNY';

        var pay_types = (showBank) ?'pay_bank':'';
            pay_types += (showAli) ?',pay_ali':'';
            pay_types += (showWechat) ?',pay_wechat':'';
            pay_types =  pay_types.indexOf(',') === 0 ? pay_types.slice(1) : pay_types;
		
        if(_this.hasClass("order-opened")){
            _this.removeClass("order-opened");
