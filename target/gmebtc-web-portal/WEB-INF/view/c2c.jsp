                dtlBank += "<div class=uppay-tip-con>"+unpayTipWords+"</div>";
                dtlBank += "<ul class=bank-dtl-ul><h3><img src='../images/pay_type_icon2.svg'><span>银行转账</span></h3>";
                dtlBank += "<li><b>卖家姓名</b>: " + its_realname + "<span class=js-copy data-clipboard-text=" + its_realname + ">复制</span></li>";
                dtlBank += "<li><b>银行名</b>: " + rowData[rowNum].bankname + "<span class=js-copy data-clipboard-text=" + rowData[rowNum].bankname + ">复制</span></li>";
                dtlBank += "<li><b>支行</b>: " + rowData[rowNum].bankbranch + "<span class=js-copy data-clipboard-text=" + rowData[rowNum].bankbranch + ">复制</span></li>";
                dtlBank += "<li><b>银行卡号</b>: " + format_info + "<span class=js-copy data-clipboard-text=" + rowData[rowNum].bankid + ">复制</span></li>";
                dtlBank += "<li><b>付款金额</b>: <span class=money-font>￥" + total +"</span><span class=js-copy data-clipboard-text=" + format_total + ">复制</span></li>";
				dtlBank += "<li><b>付款时请备注</b>: " + orderMemo + "<span class=js-copy data-clipboard-text=" + orderMemo + ">复制</span></li>";
				
				
				dtlBank += "</ul></div>";
            }
            if (showAli) {
                dtlAli += "<div class='pay-item ali-pay-item "+pay_acpt+"'>";
                dtlAli += "<div class=uppay-tip-con>"+unpayTipWords+"</div>";
                dtlAli += "<ul class=ali-dtl-ul><div><h3><img src='../images/pay_type_icon0.svg'><span>支付宝</span></h3>";
                dtlAli += "<li><b>卖家姓名</b>: " + its_realname + "<span class=js-copy data-clipboard-text=" + its_realname + ">复制</span></li>";
                dtlAli += "<li><b>支付宝账号</b>: " + rowData[rowNum].aliname + "<span class=js-copy data-clipboard-text=" + rowData[rowNum].aliname + ">复制</span></li>";
                dtlAli += "<li><b>付款金额</b>: <span class=money-font>￥" + total +"</span><span class=js-copy data-clipboard-text=" + format_total + ">复制</span></li>";
				dtlAli += "<li><b>付款时请备注</b>: " + orderMemo + "<span class=js-copy data-clipboard-text=" + orderMemo + ">复制</span></li>";
                dtlAli += "<li><span>请留意与卖家姓名是否一致</span></li></div>";
                dtlAli += "<div class='pay-code ali-code'><h3>支付宝</h3>";
                dtlAli += '<svg xmlns="http://www.w3.org/2000/svg" width="40" height="40" viewBox="1220 866 40 40"><g fill="#fff"><path d="M1258.282 866h-36.564c-.945 0-1.718.773-1.718 1.718v36.564c0 .945.773 1.718 1.718 1.718h36.564c.945 0 1.718-.772 1.718-1.718v-36.564c0-.945-.772-1.718-1.718-1.718zm-.191 36.537c0 .854-.699 1.554-1.554 1.554h-33.074a1.558 1.558 0 0 1-1.554-1.554v-33.075c0-.854.699-1.554 1.554-1.554h33.074c.854 0 1.554.699 1.554 1.554v33.075z"/><path d="M1229.757 875.96h1.909v20.08h-1.909zM1235.896 875.96h1.909v20.08h-1.909zM1242.035 875.96h1.909v20.08h-1.909zM1248.175 875.96h1.909v20.08h-1.909z"/><path d="M1248.175 875.96h1.909v20.08h-1.909zM1242.035 875.96h1.909v20.08h-1.909zM1235.896 875.96h1.909v20.08h-1.909zM1229.757 875.96h1.909v20.08h-1.909z"/></g></svg>';
                dtlAli += "<div>收款码<br>立即查看</div></div><span class='seller-code ali-code'>支付宝收款码 ></span>";		
								
				dtlAli += "</ul></div>";
            }
            if (showWechat) {
                dtlWeChat += "<div class='pay-item wechat-pay-item "+pay_acpt+"'>";
                dtlWeChat += "<div class=uppay-tip-con>"+unpayTipWords+"</div>";
                dtlWeChat += "<ul class=wechat-dtl-ul><div><h3><img src='../images/pay_type_icon1.svg'><span>微信支付</span></h3>";
                dtlWeChat += "<li><b>卖家昵称</b>: " + its_realname + "<span class=js-copy data-clipboard-text=" + its_realname + ">复制</span></li>";
                dtlWeChat += "<li><b>微信账号</b>: " + rowData[rowNum].wename + "<span class=js-copy data-clipboard-text=" + rowData[rowNum].wename + ">复制</span></li>";
                dtlWeChat += "<li><b>付款金额</b>: <span class=money-font>￥" + total +"</span><span class=js-copy data-clipboard-text=" + format_total + ">复制</span></li>";
				dtlWeChat += "<li><b>付款时请备注</b>: " + orderMemo + "<span class=js-copy data-clipboard-text=" + orderMemo + ">复制</span></li>";
                dtlWeChat += "<li><span>请留意与卖家姓名是否一致</span></li></div>";
                dtlWeChat += "<div class='pay-code wechat-code'><h3>微信</h3>";
                dtlWeChat += '<svg xmlns="http://www.w3.org/2000/svg" width="40" height="40" viewBox="1220 866 40 40"><g fill="#fff"><path d="M1258.282 866h-36.564c-.945 0-1.718.773-1.718 1.718v36.564c0 .945.773 1.718 1.718 1.718h36.564c.945 0 1.718-.772 1.718-1.718v-36.564c0-.945-.772-1.718-1.718-1.718zm-.191 36.537c0 .854-.699 1.554-1.554 1.554h-33.074a1.558 1.558 0 0 1-1.554-1.554v-33.075c0-.854.699-1.554 1.554-1.554h33.074c.854 0 1.554.699 1.554 1.554v33.075z"/><path d="M1229.757 875.96h1.909v20.08h-1.909zM1235.896 875.96h1.909v20.08h-1.909zM1242.035 875.96h1.909v20.08h-1.909zM1248.175 875.96h1.909v20.08h-1.909z"/><path d="M1248.175 875.96h1.909v20.08h-1.909zM1242.035 875.96h1.909v20.08h-1.909zM1235.896 875.96h1.909v20.08h-1.909zM1229.757 875.96h1.909v20.08h-1.909z"/></g></svg>';
                dtlWeChat += "<div>收款码<br>立即查看</div></div><span class='seller-code wechat-code'>微信收款码 ></span>";
				
				dtlWeChat += "</ul></div>";
            }

            if (status == 'unpay') {
                statuaUnPay='unpay-con ';
            }
            itemDtl = '<div class="pay-type-con '+statuaUnPay+classSmall+'">';
            itemDtl += '<em>请选择以下方式给卖家付款</em>'+typeSelect + dtlBank + dtlAli+ dtlWeChat+'</div>';
        } else { //卖单
            dtlBuyer += "<ul class=buyer-dtl-ul>";
            dtlBuyer += "<h3>买家信息</h3>";
            dtlBuyer += "<li>买家姓名: " + its_realname + "<span class=red onclick=contact(" + orderTxid + ",\'" + contactor + "\','" + its_phone + "\','" + true + "\')>联系对方>></span></li>";
            dtlBuyer += "</ul>";

            dtlMyInfo += "<ul class=myinfo-dtl-ul>";
            dtlMyInfo += "<h3>我的收款信息</h3>";
            var myPT='';
            if (showBank) { myPT += '银行转账'; }
            if (showAli) { if (myPT !='') myPT += '，'; myPT += '支付宝'; }
			if (showWechat) { if (myPT !='') myPT += '，'; myPT += '微信'; }
            dtlMyInfo += "<li>收款方式: "+ myPT +"</li>";
			dtlMyInfo += "<li>收款时请仔细检查<span class='red'>备注信息("+orderMemo+")</span></li>";
            dtlMyInfo += "</ul>";

            itemDtl=dtlBuyer + dtlMyInfo;
            odSellCon ='od-sell-con';

            if (status == 'unpay') {
                statusColor='red';
            } else if (status == 'paid' || status == 'unconfirmed'){
                statusColor='green';
            }
        }
        //订单详情左侧结束

        if (status == 'unpay') {
            if (ifBuy) {
                op = "<button class='btn-success' onclick=_page.obj.update_push_order('"+cur_symbol+"','"+orderTxid+"','"+total+"','"+contactor+"','paid','"+pay_types+"')>我已付款</button><div class='pay-tip'><i class='caret'></i>付款成功后，请点击“我已付款”</div>";
				tips += '非自动扣款，请本人付款成功后点击 “我已付款” 。未付款并点击 “我已付款”，经核实，将会暂停账号功能。（转账时请勿填写“比特币”、“虚拟币”等信息）';
            } else {
                op = "<button class='btn-normal tooltip' title='务必查看是否已收到相应款项' onclick=_page.obj.update_push_order('"+cur_symbol+"','"+orderTxid+"','"+total+"','"+contactor+"','accepted','"+pay_types+"')>确认已收款</button>";
                tips = '请及时查看账户，确认到账后及时放币（点击确认已收款）';
            }

            status = '待付款';
        }
        else if (status == 'paid') {
            status = '已付款';
            op = "<button class=btn-warn onclick=_page.obj.update_push_order('"+cur_symbol+"','"+orderTxid+"','"+total+"','','dispute')>申请仲裁</button>";
        }
        else if (status == 'unconfirmed') {
            status = '已付款';
            op = "<button class=btn-normal onclick=_page.obj.update_push_order('"+cur_symbol+"','"+orderTxid+"','"+total+"','"+contactor+"','accepted','"+pay_types+"')>确认已收款</button>";
            tips = '请及时查看账户，确认到账后及时放币（点击确认已收款）';
        }

        else if (status == 'expired') { status = '已经过期'; statusWarnTip ='切勿付款';}
        else if (status == 'finished') { status = '已完成'; }
        else if (status == 'locked') { 
			if (!ifBuy) {
                op = "<button class='btn-normal tooltip' title='务必查看是否已收到相应款项' onclick=_page.obj.update_push_order('"+cur_symbol+"','"+orderTxid+"','"+total+"','"+contactor+"','accepted','"+pay_types+"')>确认已收款</button>";
                tips = '已申请仲裁，锁定订单。请及时查看账户，确认到账后及时放币（点击确认已收款）';
            }
			status = '已锁定'; 
		}
