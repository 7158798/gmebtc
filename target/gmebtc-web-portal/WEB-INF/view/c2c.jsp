                typeSelect += '<dt data-id="wechat"><span>微信支付 ></span><i></i></dt>';
                typeSelect += '</dl>';
            }
            var resizeTimer = null;
            $(window).off('resize').on('resize', function (){
                if (resizeTimer) clearTimeout(resizeTimer);
                resizeTimer = setTimeout(function(){
                    var itemOpendID=myPush.find(".order-opened").attr("id");
                    if(itemOpendID != undefined){
                        _page.obj.rebuild_my_transaction_table();
                        _page.obj.rebuild_my_transaction_time();
                    }
                } , 200);
            });
            //小屏幕结束
			
			var payTips = "我知道了";
			if (status == 'unpay')
				payTips = "我知道了，去支付";
				
            var ifCurrOd=_this.parents("table").attr("id")=='myPushOrder',unpayTipWords='';
            if(ifCurrOd) {
                if (status == 'unpay')
                    unpayTipWords = "<span><b>非自动扣款</b>，须本人通过该方式线下付款 <b>完成后</b>，立即点击 <b>“我已付款”</b> 按钮，完成付款确认流程。系统才会通知卖家给您放币。</span><div class='utw-btn'>" + payTips + "</div>";
                else
                    unpayTipWords="<span>此订单您已付款，<b>切勿</b>再次给卖家付款。当前信息仅供浏览。</span><div class='utw-btn'>"+payTips+"</div>";
            }
            else
                unpayTipWords="<span>此订单已关闭，<b>切勿</b>再次给卖家付款。当前信息仅供浏览。</span><div class='utw-btn'>"+payTips+"</div>";

