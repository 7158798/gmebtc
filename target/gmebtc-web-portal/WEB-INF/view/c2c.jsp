	function quickSellSubmit(curr_a, curr_b) {
		
		_page.obj.on_request_push( 'sell', curr_a, curr_b, get_element('quick_sell_rate').value, get_element('quick_sell_vol').value, get_quick_pay_type('sell'), get_element('quick_sell_oid').value );
		
