	function get_pay_type(type) {
	
		var pay_type = '';

		if (type == 'buy') {
			if (get_element('buy_pay_ali') && get_element('buy_pay_ali').checked) pay_type = 'pay_ali';
			if (get_element('buy_pay_wechat') && get_element('buy_pay_wechat').checked) pay_type += ',pay_wechat';
			if (get_element('buy_pay_bank') && get_element('buy_pay_bank').checked) pay_type += ',pay_bank';
		} else if (type == 'sell') {
			if (get_element('sell_pay_ali') && get_element('sell_pay_ali').checked) pay_type = 'pay_ali';
			 if (get_element('sell_pay_wechat') && get_element('sell_pay_wechat').checked) pay_type += ',pay_wechat';
			if (get_element('sell_pay_bank') && get_element('sell_pay_bank').checked) pay_type += ',pay_bank';
		}
		return pay_type;	
	}
	
	function quickBuySubmit(curr_a, curr_b) {
	
		_page.obj.on_request_push( 'buy', curr_a, curr_b, get_element('quick_buy_rate').value, get_element('quick_buy_vol').value, get_quick_pay_type('buy'), get_element('quick_buy_oid').value);
	
		quick_hide('0','formSub');
		
