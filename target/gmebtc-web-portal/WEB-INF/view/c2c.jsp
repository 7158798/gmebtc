		var pay_type = '';

		if (type == 'buy') {
			if (get_element('quick_buy_pay_ali') && get_element('quick_buy_pay_ali').checked) pay_type = 'pay_ali';
			if (get_element('quick_buy_pay_wechat') && get_element('quick_buy_pay_wechat').checked) pay_type += ',pay_wechat';
			if (get_element('quick_buy_pay_bank') && get_element('quick_buy_pay_bank').checked) pay_type += ',pay_bank';
		} else if (type == 'sell') {
			if (get_element('quick_sell_pay_ali') && get_element('quick_sell_pay_ali').checked) pay_type = 'pay_ali';
			 if (get_element('quick_sell_pay_wechat') && get_element('quick_sell_pay_wechat').checked) pay_type += ',pay_wechat';
			if (get_element('quick_sell_pay_bank') && get_element('quick_sell_pay_bank').checked) pay_type += ',pay_bank';
