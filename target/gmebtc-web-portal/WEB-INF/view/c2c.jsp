                        $.cookie('od'+orderTxid, 1, { path: '/' });
                });
            }
            if(targetFrom != 1) {
                $(".od-con").slideDown("fast", function () {
                    copyAndLargePic();
                    _page.obj.rebuild_my_transaction_time();
                });
