<script>
$(document).ready(function (e) {
	//var url=window.location.href;
	//if(url.indexOf('?bind_phone') > -1){
	//	$(".deposit-sec").before("<h3 class='bind-phone'>您尚未绑定手机，请先绑定手机！</h3>")
	//}

	var country_code = $('select[name="country_code"]');
	function onCountryChange() {
		var code = country_code.val();
		$('#code_tip').html("+"+code);
		$('#phone_number').val('').focus();
	}
	country_code.change(onCountryChange);

	//手机号码检测
	function phoneCheck(phone) {
		var reg_phone=/^1\d{10}$/;
		var phoneNum=$("#phone_number");
		if (phone == '') {
			phoneNum.click(function () {
				$(this).parent().removeClass("err-item");
				$(this).next('p').text("");
			}).next('p').text("手机号码不能为空").parent().addClass("err-item");
			return false;
		} else {
		    if($("#code_tip").text()=='+86'){
                if ( !reg_phone.test(phone) ) {
                    phoneNum.focus().click(function () {
                        $(this).parent().removeClass("err-item");
                        $(this).next('p').text("");
                    }).next('p').text("手机号码不正确，请检查后重新输入").parent().addClass("err-item");
                    return false;
                } else {
                    $("#voicebtn,#smsbtn").prop('disabled', false);
                    return true;
                }
            } else {
                $("#voicebtn,#smsbtn").prop('disabled', false);
                return true;
            }
		}
	}
	function smsCheck() {
		var smsVal=$("#smscode").val();
		if(smsVal != ''){
			return true;
		} else {
			$('#voicebtn').next("p").text("短信验证码不能为空");
		}
	}

	var sendTimes;
	$("#phone_number").on('input propertychange', function () {
		var phone = $('#phone_number').val();
		if(phone.length > 6){
			phoneCheck(phone);
		} else {
			$("#voicebtn,#smsbtn").prop('disabled', true);
		}
		$(this).parent().removeClass("err-item");
		$(this).next('p').text("");
		if(sendTimes == 1){
			$('#smsbtn').text("获取短信验证码");
			$('#voicebtn').text("获取电话语音验证码").next("p").text("");
		}
	}).blur(function () {
		var phone = $('#phone_number').val();
		phoneCheck(phone);
	});

	$("#smscode").on('input propertychange', function () {
		var smsVal=$(this).val();
		if(smsVal != ''){
			$('#voicebtn').next("p").text("");
		} else {
			$('#voicebtn').next("p").text("短信验证码不能为空");
		}
	});

    $('#smsbtn').click(function() {
        var self = $(this);
        var currentTime = 60;
        sendTimes = 1;
        var form = self.closest('form');
		var phoneInput=$('#phone_number'), country=$("#country_code");
		var phone = phoneInput.val();
		var realname = $('#realname').val();
		if (realname == '') {
			$('#voicebtn').next("p").text("姓名不能为空");
			return;
		}
		phoneInput.attr('readonly', true);
        //country.prop('disabled', true);
		//phoneCheck(phone);
		if(phoneCheck(phone)) {
			$("#voicebtn,#smsbtn").prop('disabled', true);
			$("#subbtn").addClass("timing");

			var timer = $.timer(function () {
				currentTime = currentTime - 1;
				self.text(currentTime + '秒后可重发');

				if (currentTime == 0) {
					resetTimer();
				}
			}, 1000, false);

			var resetTimer = function () {
				timer.stop();
				currentTime = 60;
				self.text("重获短信验证码");
				$("#voicebtn,#smsbtn").prop('disabled', false);
				phoneInput.attr('readonly', false);
                //country.prop('disabled', false);
				$("#subbtn").removeClass("timing");
			};
            var country_code = country.val();
			var json_req = {
				phone: '+' + country_code + '|' + phone,
				type: 'addphone'
			};
			var smsid = $('#smsid', form).val();

			var lang = "cn";
			var view_code = "";
			var msg = "";
			$.ajax({
				type: "post", url: "/sms/" + smsid, data: json_req,
				success: function (data, textStatus) {
					var json_res = data;
					if (json_res && json_res.result) {
						self.text(currentTime + '秒后可重发');
						timer.play();
						msg = lang == "en" ? "The SMS code has been sent to your cellphone." : "短信验证码" + json_res.msg;
						//$('#smscode', form).prop('disabled', false);
					} else {
						msg = lang == "en" ? "Send SMS faild: " + json_res.msg + ", please check your phone number at <a href='/myaccount/sms_setup'>this link</a>." : "短信发送失败。" + json_res.msg;
						//resetTimer();
						timer.play();
					}
					//apprise( "<div>"+msg+"</div>", {animate:true,textOk:"确定"}, function(r){} );
					$("#voicebtn").next("p").html(msg);
				},
				error: function () {
					//resetTimer();
					timer.play();
					$("#voicebtn").next("p").text("网络错误！请稍后重试");
				},
				complete: function () {
				}
			});
		}
        return false;
    });



    $('#voicebtn').click(function() {
        var self = $(this);

        var currentTime = 60;
        var sendTimes = 1;
        var form = self.closest('form');
		var phoneInput=$('#phone_number'), country=$("#country_code");
		var phone = phoneInput.val();

		phoneInput.attr('readonly', true);
        //country.prop('disabled', true);
		if(phoneCheck(phone)) {
			$("#voicebtn,#smsbtn").prop('disabled', true);
			$("#subbtn").addClass("timing");

			var timer = $.timer(function () {
				currentTime = currentTime - 1;
				self.text(currentTime + '秒后可重发');

				if (currentTime == 0) {
					resetTimer();
				}
			}, 1000, false);

			var resetTimer = function () {
				timer.stop();
				currentTime = 60;
				self.text("重获电话语音验证码");
				$("#voicebtn,#smsbtn").prop('disabled', false);
				phoneInput.attr('readonly', false);
                //country.prop('disabled', false);
				$("#subbtn").removeClass("timing");
			};
			var json_req = {
				phone: phone,
				type: 'addphone'
			};
			var smsid = $('#smsid', form).val();
			var lang = "cn";
			var view_code = "";
			var msg = "";
			$.ajax({
				type: "post", url: "/voiceverify/" + smsid, data: json_req,
				success: function (data, textStatus) {
					var json_res = data;
					if (json_res && json_res.result) {
						self.text(currentTime + '秒后可重发');
						timer.play();
						msg = lang == "en" ? "The voide verification code has been sent to your cellphone." : "电话语音验证码" + json_res.msg;
						//$('#smscode', form).prop('disabled', false);
					} else {
						msg = lang == "en" ? "Send voice faild: " + json_res.msg + ", please check your phone number at <a href='/myaccount/sms_setup'>this link</a>." : "电话语音拨打失败：" + json_res.msg + "，请到<a href='/myaccount/sms_setup'>这里检查手机号码设置</a>.";
						//resetTimer();
						timer.play();
					}
					$("#voicebtn").next("p").text(msg);
				},
				error: function () {
					//resetTimer();
					timer.play();
					$("#voicebtn").next("p").text("网络错误！请稍后重试");
				},
				complete: function () {
				}
			});
		}
        return false;
    });

	$("#subbtn").click(function () {
		var realname = $('#realname').val();
		if (realname == '') {
			$('#voicebtn').next("p").text("姓名不能为空");
			return;
		}
		reg = /^\d+$/;
		if(reg.test(realname)){
            $('#voicebtn').next("p").text("姓名格式不正确");
            return;
		}

		var pass = $('#fundpass').val();
		if (pass == '') {
			$('#voicebtn').next("p").text("资金密码不能为空");
			return;
		}
		var phone_number = $('#phone_number').val();
		if (phone_number == '') {
			$('#voicebtn').next("p").text("手机号码不能为空");
			return;
		}
		var smscode = $('#smscode').val();
		if (smscode == '') {
			$('#voicebtn').next("p").text("短信验证码不能为空");
			return;
		}

        reg = /[\uFF00-\uFFFF]/g; //全角符号
        if(reg.test(realname)){
            $('#voicebtn').next("p").text("姓名格式不正确");
            return;
        }

		if($(this).hasClass("timing")){
			if( smsCheck() ) {
				$("#bindPhone").submit()
			}
		} else {
			var phone = $('#phone_number').val();
			if( phoneCheck(phone) && smsCheck() ) {
				$("#bindPhone").submit()
			}
		}
	})

});

</script>



	
		
		
	
	
	
	
	
		
	
	

		
	
		
		

		
	

		
	
		
	
		
	
		
	

		
	
		
	
		
	

		
	
	
	
	
	
	
	
		
	
		
	
		
