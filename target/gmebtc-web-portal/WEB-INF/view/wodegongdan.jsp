<script>
	$(document).ready(function (e) {
		var contentTabs=$("#contentTabs"),
			currencyDiv=$(".ticket-currency"),
            depositDiv=$(".ticket-depo-txid"),
            withdrawDiv=$(".ticket-txid"),
            ticketLeft=$(".ticket-left"),
            ticketRight=$(".ticket-right"),
            tLHeight=ticketLeft[0].clientHeight,
            tRHeight=ticketRight[0].clientHeight;

        if(tLHeight < tRHeight) {
            ticketLeft.css("min-height", tRHeight)
        }

		if($.cookie("content_tabs") == 1) {
			$("#ticket_type").val('1');
            contentTabs.find("li").eq(0).removeClass("ticket-active");
            $("#withdrawItem").addClass("ticket-active");
			currencyDiv.show();
            depositDiv.hide();
			withdrawDiv.show();
			
        } else if($.cookie("content_tabs") == 2) {
			$("#ticket_type").val('2');
			contentTabs.find("li").eq(0).removeClass("ticket-active");
            $("#otherItem").addClass("ticket-active");
			currencyDiv.hide();
            withdrawDiv.hide();
            depositDiv.hide();
			
		} else {
			$("#ticket_type").val('0');
			contentTabs.find("li").eq(0).removeClass("ticket-active");
            $("#depositItem").addClass("ticket-active");
			currencyDiv.show();
            withdrawDiv.hide();
            depositDiv.show();
			
		}

        contentTabs.on("click","li",function () {
            var _this=$(this);

            _this.addClass("ticket-active").siblings().removeClass("ticket-active");
            if(_this.attr("id") == 'depositItem'){
				currencyDiv.show();
                withdrawDiv.hide();
                depositDiv.show();
                $.cookie("content_tabs",0);
				$("#ticket_type").val('0');
            } else if(_this.attr("id") == 'withdrawItem'){
				currencyDiv.show();
                depositDiv.hide();
				withdrawDiv.show();
                $.cookie("content_tabs",1);
				$("#ticket_type").val('1');
            } else {
				currencyDiv.hide();
				withdrawDiv.hide();
                depositDiv.hide();
                $.cookie("content_tabs",2);
				$("#ticket_type").val('2');
			}
            //withdrawtable.css("opacity","0").animate({opacity:"1"},200)
        });
    
		var ticketTabs=$("#ticketTabs"),
			ticketsListDiv=$("#ticketsList"),
            fQAListDiv=$("#fQAList"),
			ticketItem=$("#ticketItem"),
			fQAItem=$("#fQAItem");
			
		if($.cookie("ticket_tabs") == 1) {
			fQAItem.addClass("ticket-active");
			fQAListDiv.show();
			ticketItem.removeClass("ticket-active");
            ticketsListDiv.hide();
        } else {
			ticketItem.addClass("ticket-active");
			ticketsListDiv.show();
			fQAItem.removeClass("ticket-active");            
            fQAListDiv.hide();
		}	
		ticketTabs.on("click","li",function () {
            var _this=$(this);

            _this.addClass("ticket-active").siblings().removeClass("ticket-active");
            if(_this.attr("id") == 'ticketItem'){
				ticketsListDiv.show();
                fQAListDiv.hide();
                $.cookie("ticket_tabs",0);
            } else {
				fQAListDiv.show();
				ticketsListDiv.hide();
                $.cookie("ticket_tabs",1);
			}
        });	
		
	});
	
    $(".kyc.form").on("change", ".file", function(){
        $(this).parents("form").trigger("submit");
    });
    $.cookie('nav_index', 2,{ path: '/' });
	
	function fixedAlertPosition(_this) {
        var screenH=document.body.clientHeight,screenW=document.body.clientWidth,alertH=_this.height(),alertW=_this.width();
        _this.css({"left":(screenW-alertW)/2,"top":(screenH-alertH)/2})
    }
	
	//大图
	$(".ticket-pic1,.ticket-pic2,.ticket-pic3").click(function () {
		var imgsrc = $(this).attr("src");
		$("<div class=img-modal><img src=" + imgsrc + "><span class=img-close>×</span></div>").prependTo("body");
		var imgModal = $(".img-modal");
		fixedAlertPosition(imgModal);
		$(window).on("resize", function () {
			fixedAlertPosition(imgModal);
		});
		$(".img-close").click(function () {
			$(".img-modal").remove();
			$(window).off("resize");
		});
		var timer = setTimeout(function () {
			$(".img-close").stop().animate({opacity: '0'})
		}, 1000);
		imgModal.hover(function () {
			$(".img-close").stop().animate({opacity: '1'});
			clearTimeout(timer)
		}, function () {
			$(".img-close").stop().animate({opacity: '0'})
		});

	});
	function  OnResetUpload() {

		noty({
				text: '是否重新上传',
				type: 'confirm',
				layout: 'center',
				theme: 'gateioNotyTheme',
				modal: true,
				animation: {
					open: {height: 'toggle'},
					close: {height: 'toggle'},
					easing: 'swing',
					speed: 100 // opening & closing animation speed
				},
				buttons: [
					{
						addClass: 'btn btn-primary dp-noty-btn', text: '确定', onClick: function ($noty) {

							document.getElementById('reset-form').submit();
							$noty.close();
							return true;
						}
					},
					{
						addClass: 'btn btn-danger dp-noty-btn', text: '关闭', onClick: function ($noty) {
							$noty.close();
							return false;
						}
					}
				]
		});
	}

	function OnUserClose(id) {
	
		var uid = '1581772';
		if (id !== undefined && id > 0){
			noty({
				text: '确定关闭工单',
				type: 'confirm',
				layout: 'center',
				theme: 'gateioNotyTheme',
				modal: true,
				animation: {
					open: {height: 'toggle'},
					close: {height: 'toggle'},
					easing: 'swing',
					speed: 100 // opening & closing animation speed
				},
				buttons: [
					{
						addClass: 'btn btn-primary dp-noty-btn', text: '确定', onClick: function ($noty) {
						
							var json_req = {
								type : "close_ticket",
								uid : uid,
								id : id
							};
							$.ajax({type:"get",
								url: "/json_svr/tickets/?u=13" + "&c=" + Math.floor(Math.random() * 1000000),
								data:json_req, xhrFields: { withCredentials: true},
								success : function(data, textStatus){
									var json_res = data;
									if ( json_res && json_res.result ) {
										window.location = '/myaccount/tickets';
									}else{
										if (json_res) $('#ticket_tips').html('<span class=red>'+json_res.msg+'</span>');
										else  $('#ticket_tips').html('<span class=red>系统错误，请稍后再试</span>');
									}
								}
							});
							$noty.close();
							return true;
						}
					},
					{
						addClass: 'btn btn-danger dp-noty-btn', text: '取消', onClick: function ($noty) {
							$noty.close();
							return false;
						}
					}
				]
			});
		}
	}
</script>



	
	
	
		
		
		
		
		
	
	
		
		
	
	
	
	
	
		
	
	

		
	
		
		

		
	

		
	
		
	
		
	
		
	

		
	
		
	
		
	

		
	
	
	
	
	
	
	
		
	
		
	
		
