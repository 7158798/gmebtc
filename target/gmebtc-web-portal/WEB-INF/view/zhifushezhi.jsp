    });
	
	function initPaymentSetup() {
		var enablezerobank = $("input[name='enablezerobank']");
		var bind_bank_disabled = '1';
		
		if (bind_bank_disabled === undefined || bind_bank_disabled === '0') {
			enablezerobank.prop('checked', true).addClass('zero-active');
			$('#bindBankDiv').show();
		} else { 
			enablezerobank.prop('checked', false).removeClass('zero-active');
			$('#bindBankDiv').hide();
		}
		
		var enablezeroali = $("input[name='enablezeroali']");
		var bind_ali_disabled = '0';
		
		if (bind_ali_disabled === undefined || bind_ali_disabled === '0') {
			enablezeroali.prop('checked', true).addClass('zero-active');
			$('#bindAliDiv').show();
		} else { 
			enablezeroali.prop('checked', false).removeClass('zero-active');
			$('#bindAliDiv').hide();
		}
		
		var enablezerowechat = $("input[name='enablezerowechat']");
		var bind_wechat_disabled = '1';
		
		if (bind_wechat_disabled === undefined || bind_wechat_disabled === '0') {
			enablezerowechat.prop('checked', true).addClass('zero-active');
			$('#bindWechatDiv').show();
		} else { 
			enablezerowechat.prop('checked', false).removeClass('zero-active');
			$('#bindWechatDiv').hide();
		}
	}
	
    function OnDisablePayment(obj, payment_type)
    {
        var disable = 0;
		if (payment_type == 'bank')
			disable = $('#enablezerobank').is(':checked')==false?1:0;
		else if (payment_type == 'alipay')
			disable = $('#enablezeroali').is(':checked')==false?1:0;
		else if (payment_type == 'wechat')
			disable = $('#enablezerowechat').is(':checked')==false?1:0;
			
		var uid = $('#bind_uid').val();
        var json_req = {
            type : "disable_payment",
            uid : uid,
            disable: disable,
            payment_type : payment_type
        };
		
        $.ajax({type:"get",
            url: "/json_svr/exchange/?u=13" + "&c=" + Math.floor(Math.random() * 1000000),
            data:json_req, xhrFields: { withCredentials: true},
            success : function(data, textStatus){
                var json_res = data;
				var enablezero, binddiv;
				
				if (payment_type == 'bank') {
					enablezero = $("input[name='enablezerobank']");
					binddiv = $('#bindBankDiv');
				}
				else if (payment_type == 'alipay') {
					enablezero = $("input[name='enablezeroali']");
					binddiv = $('#bindAliDiv');
				}
				else if (payment_type == 'wechat') {
					enablezero = $("input[name='enablezerowechat']");
					binddiv = $('#bindWechatDiv');
				}
				
                if ( json_res && json_res.result ) {		
				
						if (json_res.bind_result == '0') {
							binddiv.show();
							setTimeout(function(){enablezero.addClass('zero-active')},500);
						} else if (json_res.bind_result == '1') {
							binddiv.hide();
							setTimeout(function(){enablezero.removeClass('zero-active')},500);
						}
					
                }else{
					
					if (json_res)
						noty({text: json_res.msg, type: "error",layout: "center", theme: 'gateioNotyTheme', timeout: 5000});						
					else 
						noty({text: '系统错误，请稍后再试', type: "error",layout: "center", theme: 'gateioNotyTheme', timeout: 5000});											
					
					if (disable) 
						setTimeout(function(){enablezero.prop('checked', true).addClass('zero-active')},200);
					else 
						setTimeout(function(){enablezero.prop('checked', false).removeClass('zero-active')},200);
                }
				setTimeout(function(){$(obj).removeAttr("disabled");},1000);
            }
        });
        $(obj).attr("disabled","disabled");
    }
	
	function resizeLeftbar() {
		var lb=$(".leftbar"), mc=$(".main_content"),lh=lb.height(),mh=mc.height();
		lb.css("height",mh)
	}

    function addNewBankAddr() {

        var realname = $('#realname').val();
        if (realname == '') {
            noty({text: '汇款人姓名不能为空', type: "warning",layout: "center", theme: 'gateioNotyTheme', timeout: 5000});
            return;
        }

        $('#bindbankaddr').removeClass('hide');
        $('#bindCardTable').addClass('hide');
		resizeLeftbar()

    }

