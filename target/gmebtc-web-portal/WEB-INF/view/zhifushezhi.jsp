                    if (json_res) $('#bind_bank_tips').html('<span class=red>'+json_res.msg+'</span>');
                    else  $('#bind_bank_tips').html('<span class=red>系统错误，请稍后再试</span>');
                    setTimeout(function(){$('#bindBankBtn').attr("disabled",false);},1000);
                }
            }
        });
        $('#bindBankBtn').attr("disabled",true);
    }

    function initializeAliCodeView() {
		var sfzImg=$("#aliCodeImgBox");
