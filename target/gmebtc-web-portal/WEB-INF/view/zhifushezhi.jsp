    function OnCloseBindBank()
    {
        $('#bindbankaddr').removeClass('hide');
        $('#bindCardTable').addClass('hide');
		resizeLeftbar()
    }

    function OnBindBank()
    {
        var bank_name = $('#bank_name').val();
        if (bank_name == '') {
            $('#bind_bank_tips').html('<span class=red>请选择开户银行</span>');
            return;
        }

        var bank_branch = $('#bank_branch').val();
        if (bank_branch == '') {
            $('#bind_bank_tips').html('<span class=red>请填写开户支行</span>');
            return;
        }

        var bank_addr = $('#bank_addr').val();
        if (bank_addr == '') {
            $('#bind_bank_tips').html('<span class=red>请填写银行卡号</span>');
            return;
        }
        var can_bind_bank = $('#can_bind_bank').val();
        if (can_bind_bank == 0) {
            $('#bind_bank_tips').html('<span class=red>仅限绑定中国大陆身份证用户进行银行卡绑定</span>');
            return;
        }
        var fun_pwd = $('#fund_pwd_bank').val();
        if (fun_pwd == '') {
            $('#bind_bank_tips').html('<span class=red>请填写资金密码</span>');
            return;
        }
		var agr=$("#agrCheckLabel");
		if(!$("#agreeCheck").prop("checked")){
			$('#bind_bank_tips').html('<span class=red>请认真阅读风险提示</span>');
            return;
		}
        var uid = $('#bind_uid').val();
		var bankid = $('#bank_id').val();
        var json_req = {
            type : "bind_bank",
            uid : uid,
            bankid: bankid,
            bank_name : bank_name,
            bank_branch : bank_branch,
            fund_pwd : fun_pwd,
            bank_addr : bank_addr
        };
        $.ajax({type:"get",
            url: "/json_svr/exchange/?u=13" + "&c=" + Math.floor(Math.random() * 1000000),
            data:json_req, xhrFields: { withCredentials: true},
            success : function(data, textStatus){
