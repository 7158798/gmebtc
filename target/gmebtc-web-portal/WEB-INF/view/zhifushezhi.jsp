                }else{
                    if (json_res) $('#bind_ali_tips').html('<span class=red>'+json_res.msg+'</span>');
                    else  $('#bindAli_tips').html('<span class=red>系统错误，请稍后再试</span>');
					setTimeout(function(){$('#bindAliBtn').attr("disabled",false);},1000);
                }
            },
            error: function (req, textStatus, err) {

            }
        });
        $('#bindAliBtn').attr("disabled",true);

    }
	
	function initializeWechatCodeView() {
		var sfzImg=$("#wechatCodeImgBox");
		if(sfzImg.find("img").attr("src") != ''){
			sfzImg.css('background-image','none').siblings(".re-select").show();
			setTimeout(function () {
				$("#reSelectBtn_wechat").css("margin-top",(sfzImg.height()-36)/2);
			},200);
		} else {
			sfzImg.find("img").css("opacity","0")
		}
	}

	function addNewWechat() {

        var realname = $('#realname').val();
        if (realname == '') {
            noty({text: '姓名不能为空', type: "warning",layout: "center", theme: 'gateioNotyTheme', timeout: 5000});
            return;
        }

        $('#bindwechataddr').removeClass('hide');
        $('#bindWechatTable').addClass('hide');

		initializeWechatCodeView();
		resizeLeftbar()
    }

    function editWechat() {
        $('#bindwechataddr').removeClass('hide');
        $('#bindWechatTable').addClass('hide');
		initializeWechatCodeView();
		resizeLeftbar()
	}

    function OnCloseBindWechat()
    {
        $('#bindWechatTable').removeClass('hide');
        $('#bindwechataddr').addClass('hide');
		resizeLeftbar()
    }

    function OnBindWechat()
    {

        var wechat_name = $('#wechat_name').val();
        if (wechat_name == '') {
            $('#bindWechat_tips').html('<span class=red>请填写微信账户</span>');
            return;
        }

        var fund_pwd = $('#fund_pwd_wechat').val();
        if (fund_pwd == '') {
            $('#bind_wechat_tips').html('<span class=red>请填写资金密码</span>');
            return;
        }

        var wechat_id = $('#wechat_id').val();
        var uid = $('#bind_uid').val();
		var wechat_memo = $('#wechat_memo').val();
		wechat_memo = wechat_memo.replace(/[\r\n]/g, "");
        var json_req = {
            type : "bind_wechat",
            uid : uid,
            wechat_name : wechat_name,
            wechat_id : wechat_id,
            fund_pwd : fund_pwd,
			wechat_memo : wechat_memo
        };
        $.ajax({type:"get",
            url: "/json_svr/exchange/?u=13" + "&c=" + Math.floor(Math.random() * 1000000),
            data:json_req, xhrFields: { withCredentials: true},
            success : function(data, textStatus){
                var json_res = data;
                if ( json_res && json_res.result ) {
                    $('#bindWechat_tips').html('<span class=green>绑定成功</span>');
                    window.location.reload();
                }else{
                    if (json_res) $('#bind_wechat_tips').html('<span class=red>'+json_res.msg+'</span>');
                    else  $('#bindWechat_tips').html('<span class=red>系统错误，请稍后再试</span>');
					setTimeout(function(){$('#bindWechatBtn').attr("disabled",false);},1000);
                }
            },
            error: function (req, textStatus, err) {

            }
        });
        $('#bindWechatBtn').attr("disabled",true);

    }

	$(".re-select").click(function () {
	 	$(this).prev(".file").click()
	});

</script>



		
		
	
	
	
		
		
		
		
		
	
	
		
		
	
	
	
	
	
		
	
	

		
	
		
		

		
	

		
	
		
	
		
	
		
	

		
	
		
	
		
	

		
	
	
	
	
	
	
	
		
	
		
	
		
